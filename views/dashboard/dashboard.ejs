<h1>Hi <%= locals.name %> </h1>
<form action="/dashboard/logout?_method=DELETE" method="POST">
    <button type="submit">Log Out</button>
</form>
<% if(locals.bundle) { %>
<a href="/dashboard/bundle">Dein Bundle</a>
<form action="/dashboard/leave?_method=DELETE" method="POST">
    <button type="submit">Leave bundle</button>
</form>
<% } else {%>
    <a href="/bundles">Kauf dir ein Bundle</a>
<% } %>